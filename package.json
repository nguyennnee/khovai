{
  "name": "lil-shunshine-thrift-fullstack",
  "version": "1.0.0",
  "description": "Full stack application for Lil Sunshine Thrift Shop",
  "main": "start_fullstack.js",
  "scripts": {
    "setup": "node setup_fullstack.js",
    "start": "node start_fullstack.js",
    "start:api": "cd api && php start_api.php",
