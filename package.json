    "install:frontend": "cd lil-shunshine-thrift-frontend && npm install",
    "build:frontend": "cd lil-shunshine-thrift-frontend && npm run build",
    "test": "node test_connection.js",
    "test:api": "node test_connection.js"
  },
  "keywords": [
    "thrift",
    "vintage",
    "fashion",
    "ecommerce",
